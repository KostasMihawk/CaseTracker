@model IEnumerable<CaseTracker.Models.Case>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Attorney.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Court.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Aa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfAssignment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfSubmission)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfEnd)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Attorney.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Court.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Aa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfAssignment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfSubmission)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfEnd)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
