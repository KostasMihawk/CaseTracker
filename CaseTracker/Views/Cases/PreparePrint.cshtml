@model CaseTracker.Models.Case

@{
	ViewBag.Title = "Details";
	Layout = "";
}
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@Styles.Render("~/Content/css")
<h2>Details for case: @Html.DisplayFor(model => model.Aa)</h2>

<div>
	<dl class="dl-horizontal">
		<dt>
			@Html.DisplayNameFor(model => model.Aa)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.Aa)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.Attorney.FullName)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.Attorney.FullName)
		</dd>

		<dt>
			@Html.DisplayNameFor(model => model.Court.Name)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.Court.Name)
		</dd>

		<dt>
			@Html.DisplayNameFor(model => model.DocumentType.Description)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.DocumentType.TranslatedDescription)
		</dd>

		<dt>
			@Html.DisplayNameFor(model => model.DateOfAssignment)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.DateOfAssignment)
		</dd>

		<dt>
			@Html.DisplayNameFor(model => model.DateOfSubmission)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.DateOfSubmission)
		</dd>

		<dt>
			@Html.DisplayNameFor(model => model.DateOfEnd)
