		<dd>
			@Html.DisplayFor(model => model.DateOfEnd)
		</dd>

		<dt>
			@Html.DisplayNameFor(model => model.Notes)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.Notes)
		</dd>

		<dt>
			@Html.DisplayNameFor(model => model.ProsecutionId)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.Prosecution.FullName)
		</dd>

		<dt>
			@Html.DisplayNameFor(model => model.DefenseId)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.Defense.FullName)
		</dd>

		<dt>
			@Html.DisplayNameFor(model => model.RecipientId)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.Recipient.FullName)
		</dd>

